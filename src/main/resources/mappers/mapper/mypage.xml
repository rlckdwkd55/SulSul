<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sulsulmarket.sulsul.mypage.mapper.MypageMapper">

    <select id="getOrderList" resultType="com.sulsulmarket.sulsul.mypage.dto.Orders">
        SELECT * FROM ORDERS WHERE MEMBER_ID LIKE CONCAT(#{MEMBER_ID})
    </select>

    <select id="getReviewList" resultType="com.sulsulmarket.sulsul.mypage.dto.Review">
        SELECT * FROM REVIEW WHERE MEMBER_ID LIKE CONCAT(#{MEMBER_ID})
    </select>

</mapper>